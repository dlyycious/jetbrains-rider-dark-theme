{
  "name": "JetBrains Rider Dark Theme",
  "type": "dark",
  "colors": {
    "titleBar.activeBackground": "#2B2D30",
    "titleBar.inactiveBackground": "#2B2D30",
    "titleBar.activeForeground": "#BDC0C9",
    "titleBar.inactiveForeground": "#BDC0C9",
    "activityBar.background": "#2B2D30",
    "activityBar.foreground": "#BDC0C9",
    "activityBar.border": "#1E1F21",
    "sideBarTitle.foreground": "#BDC0C9",
    "sideBar.background": "#2B2D30",
    "sideBar.foreground": "#BDC0C9",
    "sideBar.border": "#1E1F21",
    "sideBarSectionHeader.background": "#2B2D30",
    "sideBarSectionHeader.foreground": "#BDC0C9",
    "sideBarSectionHeader.border": "#1E1F21",
    "tab.activeBackground": "#1E1F21",
    "tab.activeForeground": "#82ACFC",
    "tab.activeBorder": "#3474F0",
    "tab.inactiveBackground": "#1E1F21",
    "tab.inactiveForeground": "#9FA1A5",
    "tab.border": "#1E1F21",
    "toolbar.activeBackground": "#1E1F21",
    "breadcrumb.background": "#1E1F21",
    "breadcrumb.foreground": "#BDC0C9",
    "widget.shadow": "#00000075",
    "editorWidget.background": "#2B2D30",
    "editorWidget.border": "#2B2D30",
    "editorWidget.foreground": "#BDC0C9",
    "scrollbar.shadow": "#262626",
    "editor.background": "#262626",
    "editor.foreground": "#BDC0C9",
    "editor.selectionBackground": "#0A3783",
    "editorGroup.emptyBackground": "#1E1F21",
    "editorGroupHeader.tabsBackground": "#1E1F21",
    "editorGroupHeader.tabsBorder": "#393B41",
    "editorGroup.border": "#393B41",
    "panel.background": "#262626",
    "panel.border": "#1E1F21",
    "panelTitle.activeBorder": "#3474F0",
    "panelTitle.activeForeground": "#82ACFC",
    "panelTitle.inactiveForeground": "#9FA1A5",
    "statusBar.background": "#2B2D30",
    "statusBar.foreground": "#BDC0C9",
    "statusBar.debuggingBackground": "#3474F0",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.noFolderBackground": "#2B2D30",
    "statusBar.noFolderForeground": "#BDC0C9",
    "button.background": "#3474F0",
    "list.activeSelectionBackground": "#0A3783",
    "list.inactiveSelectionBackground": "#2E436B",
    "list.hoverBackground": "#2E436B"
  },
  "tokenColors": [
    {
      "scope": [
        "keyword",
        "storage.type",
        "constant.language.boolean",
        "storage.modifier.async",
        "variable.language.this",
        "constant.language"
      ],
      "settings": {
        "foreground": "#6C95EB"
      }
    },
    {
      "scope": [
        "entity.name.type.namespace",
        "entity.name.type.interface",
        "entity.name.type.class",
        "support.type.primitive",
        "support.class",
        "support.type.exception"
      ],
      "settings": {
        "foreground": "#C191FF"
      }
    },
    {
      "scope": ["entity.name.type.enum", "entity.name.type.struct", "entity.name.type"],
      "settings": {
        "foreground": "#E2BFFF"
      }
    },
    {
      "scope": [
        "entity.name.variable.enum-member",
        "entity.name.variable.field",
        "entity.name.variable.property",
        "variable.other.object.property",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#65C3CC"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#85C46C"
      }
    },
    {
      "scope": ["entity.name.function", "support.function.magic", "support.function.builtin"],
      "settings": {
        "foreground": "#3ACC9C"
      }
    },
    {
      "scope": [
        "string.quoted.double",
        "string.interpolated.double",
        "string.quoted.single",
        "meta.preprocessor.include"
      ],
      "settings": {
        "foreground": "#C8A16D"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "meta.interpolation",
        "entity.name.variable.local",
        "punctuation.parenthesis.open",
        "punctuation.parenthesis.close",
        "punctuation.terminator.statement",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#BDBDBD"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#EC94C0"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator": "#7488C3",
    "stringLiteral": "#6CAB74",
    "customLiteral": "#BDC0C9",
    "numberLiteral": "#BDC0C9"
  }
}
